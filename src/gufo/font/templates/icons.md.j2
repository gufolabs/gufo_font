---
hide:
    - navigation
    - toc
---
<link rel="stylesheet" href="../assets/icons.css">
<script src="../assets/icons.js"></script>


<div class="icons-search">
  <label for="icons-search-input">Search icons:</label>
  <input
    type="search"
    id="icons-search-input"
    name="icons-search"
    placeholder="Type to filter icons"
    autocomplete="off"
    autofocus
    style="border-bottom: 1px solid var(--md-typeset-color);width: 80%; padding: 4px;"
  >
</div>

<div class="icons-empty icons-wrapper" hidden>Not found</div>

{% for group in manifest.icons %}
## {{group}}
  <div class="icons-list icons-wrapper">
  {% for icon in manifest.iter_enabled_icons(group) %}
  <div class="icon">
    <i class="gf {{icon.name}}"></i>
    <div class="title">{{icon.name}}</div>
  </div>
  {% endfor %}
  </div>
{% endfor %}
